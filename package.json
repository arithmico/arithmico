  "scripts": {
    "start:desktop": "npm run build:offline -w @arithmico/calc && cp -r packages/arithmico-calc/dist packages/arithmico-desktop/web-dist && npm run start -w @arithmico/desktop",
    "start:config": "npm start -w @arithmico/config",
    "start:calc": "npm start -w @arithmico/calc",
    "start:docs": "npm start -w @arithmico/docs",
    "start:blog": "npm start -w @arithmico/blog",
    "build:all": "npm run build -w @arithmico/engine -w @arithmico/calc -w @arithmico/docs -w @arithmico/blog -w @arithmico/config",
    "build:parser": "npm run build -w @arithmico/parser",
    "build:engine": "npm run build -w @arithmicobs/engine",
    "rebuild:engine": "npm run build -w @arithmico/parser && npm run build -w @arithmico/engine",
    "build:calc": "npm run build -w @arithmico/calc",
    "build:docs": "npm run build -w @arithmico/docs",
    "build:blog": "npm run build -w @arithmico/blog",
    "build:config": "npm run build -w @arithmico/config",
    "test:engine": "npm run test -w @arithmico/engine",
    "retest:engine": "npm run rebuild:engine && npm run test:engine",
    "test:calc": "npm run test -w @arithmico/calc",
    "test:docs": "npm run test -w @arithmico/docs",
    "test:blog": "npm run test -w @arithmico/blog",
    "test:config": "npm run test -w @arithmico/config",
    "test:all": "npm run test -ws",
    "lint:calc": "npm run lint -w @arithmico/calc",
    "lint:docs": "npm run lint -w @arithmico/docs",
    "lint:blog": "npm run lint -w @arithmico/blog",
    "lint:config": "npm run lint -w @arithmico/config",
    "lint:engine": "npm run lint -w lib@arithmicos/engine",
    "lint:all": "npm run lint -ws"
  },
